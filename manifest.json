{
  "manifest_version": 2,

  "name": "Bittrex extender",
  "description": "Bittrex extender",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://bittrex.com/*/*" ],
      "css": ["styles.css"],
      "js": ["jquery-3.2.1.min.js", "scripts.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}